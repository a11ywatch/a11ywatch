{
  "name": "angelica",
  "version": "0.0.1",
  "private": true,
  "description": "find a11y issues and add scripts",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "nodemon -e js,ts,graphql -r ts-node/register -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "lint": "eslint .",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "node dist/server.js"
  },
  "engines": {
    "node": "v12.x"
  },
  "dependencies": {
    "@a11ywatch/log": "^0.2.9",
    "@a11ywatch/website-source-builder": "^0.0.4",
    "body-parser": "^1.19.0",
    "cors": "2.8.0",
    "date-fns": "2.7.0",
    "dotenv": "8.2.0",
    "express": "4.14.0",
    "generic-pool": "3.7.1",
    "get-page-speed": "^1.0.1",
    "node-fetch": "2.6.0",
    "pa11y": "5.3.0",
    "prettier": "^2.0.4",
    "puppeteer": "7.1.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.10.2",
    "@types/pa11y": "^5.3.0",
    "babel-jest": "^26.6.3",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.4.1",
    "jest": "^26.4.2",
    "jest-config": "^26.6.3",
    "nodemon": "1.9.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "6.2.2",
    "ts-node": "8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tscpaths": "0.0.9",
    "typescript": "3.8.3",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "keywords": [
    "accessibility",
    "testing",
    "WCAG"
  ]
}
