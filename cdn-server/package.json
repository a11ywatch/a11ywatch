{
  "name": "cdn-server",
  "version": "1.0.0",
  "description": "cdn server for a11ywatch",
  "main": "./dist/server.js",
  "author": "Jeffrey Mendez",
  "scripts": {
    "test": "jest",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "dev": "nodemon -e js,ts -r ts-node/register src/server.ts",
    "start": "NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@a11ywatch/log": "^0.3.4",
    "aws-sdk": "^2.666.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.0",
    "express": "4.14.0",
    "uglify-js": "^3.7.4"
  },
  "engines": {
    "node": "14.x"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.2",
    "jest": "^26.4.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.0",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "accessibility",
    "testing",
    "WCAG"
  ]
}
